{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "semi": 2,
    "no-unused-vars": ["warn"],
    "no-debugger": ["warn"],
  },
  "globals": {
    "$": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
